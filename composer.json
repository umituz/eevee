{
    "name": "umituz/eeve",
    "description": "Eeve, flexible micro service management",
    "version": "1.0.15",
    "keywords": [
        "umituz",
        "eeve"
    ],
    "homepage": "https://github.com/umituz/eeve",
    "license": "MIT",
    "authors": [
        {
            "name": "Ãœmit UZ",
            "email": "info@umituz.com",
            "homepage": "https://umituz.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-pdo": "*",
        "hisorange/browser-detect": "^5.0",
        "laravel/framework": "^11.0",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-permission": "^6.7",
        "spatie/laravel-medialibrary": "^11.4",
        "spatie/laravel-translatable": "^6.6",
        "intervention/image": "^2",
        "rap2hpoutre/laravel-log-viewer": "^2.4",
        "orangehill/iseed": "^3.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.0",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^10.5",
        "spatie/laravel-ignition": "^2.4"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "Eeve\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eeve\\Providers\\EeveServiceProvider",
                "Spatie\\Permission\\PermissionServiceProvider",
                "Spatie\\MediaLibrary\\MediaLibraryServiceProvider",
                "Spatie\\Translatable\\TranslatableServiceProvider",
                "Intervention\\Image\\ImageServiceProvider",
                "Rap2hpoutre\\LaravelLogViewer\\LaravelLogViewerServiceProvider",
                "Orangehill\\Iseed\\IseedServiceProvider"
            ]
        },
        "aliases": {
            "Image": "Intervention\\Image\\Facades\\Image"
        }
    }
}
