{
    "name": "umituz/eeve",
    "description": "Eeve, flexible micro service management",
    "version": "1.0.7",
    "keywords": [
        "umituz",
        "eeve"
    ],
    "homepage": "https://github.com/umituz/eeve",
    "license": "MIT",
    "authors": [
        {
            "name": "Ãœmit UZ",
            "email": "info@umituz.com",
            "homepage": "https://umituz.com",
            "role": "Developer"
        }
    ],
    "require": {
        "spatie/laravel-permission": "^6.7",
        "spatie/laravel-medialibrary": "^10.0",
        "spatie/laravel-translatable": "^6.6",
        "intervention/image": "^2"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "Eeve\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eeve\\Providers\\EeveServiceProvider",
                "Spatie\\Permission\\PermissionServiceProvider",
                "Spatie\\MediaLibrary\\MediaLibraryServiceProvider",
                "Spatie\\Translatable\\TranslatableServiceProvider",
                "Intervention\\Image\\ImageServiceProvider"
            ]
        },
        "aliases": {
            "Image": "Intervention\\Image\\Facades\\Image"
        }
    }
}
